<?xml version="1.0"?>

<system name="fuel">

	<channel name="fuel">

		<!-- JETTISON -->

		<switch name="propulsion/jettison">
			<default value="0" />
			<test logic="AND" value="1">
				<test logic="OR">
					/controls/fuel/jettison/valve[0] == 1
					/controls/fuel/jettison/valve[1] == 1
				</test>
				/controls/fuel/jettison/selector gt 0
				propulsion/total-fuel-lbs gt /controls/fuel/jettison/fuel-to-remain-lbs
			</test>
		</switch>

		<switch name="propulsion/tank[0]/external-flow-rate/jettison[0]">
			<default value="0" />
			<test logic="AND" value="16.5">
				propulsion/jettison == 1
				propulsion/tank[0]/override-pump[0]/psi gt 0
				propulsion/tank[0]/contents-lbs gt 7055
			</test>
		</switch>
		<switch name="propulsion/tank[0]/external-flow-rate/jettison[1]">
			<default value="0" />
			<test logic="AND" value="16.5">
				propulsion/jettison == 1
				propulsion/tank[0]/override-pump[1]/psi gt 0
				propulsion/tank[0]/contents-lbs gt 7055
			</test>
		</switch>

		<switch name="propulsion/tank[1]/external-flow/tank[2]">
			<default value="0" />
			<test logic="AND" value="1">
				<test logic="OR">
					propulsion/jettison == 1
					/controls/fuel/transfer-main-1-4 == 1
				</test>
				<test logic="OR">
					propulsion/tank[2]/contents-lbs lt 20000
					propulsion/tank[3]/contents-lbs lt 20000
				</test>
			</test>
		</switch>
		<switch name="propulsion/tank[1]/external-flow-rate/tank[2]">
			<default value="0" />
			<test logic="AND" value="25">
				propulsion/tank[1]/external-flow/tank[2] == 1
				propulsion/tank[1]/contents-lbs gt 0.0
				propulsion/tank[2]/pct-full lt 99.9
			</test>
		</switch>

		<switch name="propulsion/tank[4]/external-flow/tank[3]">
			<default value="0" />
			<test logic="AND" value="1">
				<test logic="OR">
					propulsion/jettison == 1
					/controls/fuel/transfer-main-1-4 == 1
				</test>
				<test logic="OR">
					propulsion/tank[2]/contents-lbs lt 20000
					propulsion/tank[3]/contents-lbs lt 20000
				</test>
			</test>
		</switch>
		<switch name="propulsion/tank[4]/external-flow-rate/tank[3]">
			<default value="0" />
			<test logic="AND" value="25">
				propulsion/tank[4]/external-flow/tank[3] == 1
				propulsion/tank[4]/contents-lbs gt 0.0
				propulsion/tank[3]/pct-full lt 99.9
			</test>
		</switch>

		<switch name="propulsion/tank[2]/external-flow-rate/jettison">
			<default value="0" />
			<test logic="AND" value="33">
				propulsion/jettison == 1
				<test logic="OR">
					propulsion/tank[2]/override-pump[0]/psi gt 0
					propulsion/tank[2]/override-pump[1]/psi gt 0
				</test>
				propulsion/tank[2]/contents-lbs gt 7055
			</test>
		</switch>
		<switch name="propulsion/tank[3]/external-flow-rate/jettison">
			<default value="0" />
			<test logic="AND" value="33">
				propulsion/jettison == 1
				<test logic="OR">
					propulsion/tank[3]/override-pump[0]/psi gt 0
					propulsion/tank[3]/override-pump[1]/psi gt 0
				</test>
				propulsion/tank[3]/contents-lbs gt 7055
			</test>
		</switch>
		<switch name="propulsion/tank[7]/external-flow-rate/jettison">
			<default value="0" />
			<test logic="AND" value="33">
				propulsion/jettison == 1
				propulsion/tank[7]/contents-lbs gt 7055
			</test>
		</switch>
		<summer name="propulsion/fuel-dump-rate-pps">
			<input>propulsion/tank[0]/external-flow-rate/jettison[0]</input>
			<input>propulsion/tank[0]/external-flow-rate/jettison[1]</input>
			<input>propulsion/tank[2]/external-flow-rate/jettison</input>
			<input>propulsion/tank[3]/external-flow-rate/jettison</input>
			<input>propulsion/tank[7]/external-flow-rate/jettison</input>
		</summer>
		<fcs_function name="propulsion/fuel-dump-time-sec">
			<function>
				<quotient>
					<sum>
						<property>propulsion/total-fuel-lbs</property>
						<property>-/controls/fuel/jettison/fuel-to-remain-lbs</property>
					</sum>
					<property>propulsion/fuel-dump-rate-pps</property>
				</quotient>
			</function>
		</fcs_function>

		<!-- PUMPS -->

		<switch name="propulsion/tank[0]/override-pump[0]/psi">
			<default value="0" />
			<test logic="AND" value="45">
				/controls/fuel/tank[0]/override-pump[0] == 1
				propulsion/tank[0]/contents-lbs ge 2000
			</test>
		</switch>
		<switch name="propulsion/tank[0]/override-pump[1]/psi">
			<default value="0" />
			<test logic="AND" value="45">
				/controls/fuel/tank[0]/override-pump[1] == 1
				propulsion/tank[0]/contents-lbs ge 2000
			</test>
		</switch>
		<switch name="propulsion/tank[2]/override-pump[0]/psi">
			<default value="0" />
			<test logic="AND" value="45">
				/controls/fuel/tank[2]/override-pump[0] == 1
				<test logic="OR">
					propulsion/tank[0]/contents-lbs le 3000
					propulsion/jettison == 1
				</test>
				propulsion/tank[2]/contents-lbs ge 7000
			</test>
		</switch>
		<switch name="propulsion/tank[2]/override-pump[1]/psi">
			<default value="0" />
			<test logic="AND" value="45">
				/controls/fuel/tank[2]/override-pump[1] == 1
				<test logic="OR">
					propulsion/tank[0]/contents-lbs le 3000
					propulsion/jettison == 1
				</test>
				propulsion/tank[2]/contents-lbs ge 7000
			</test>
		</switch>
		<switch name="propulsion/tank[3]/override-pump[0]/psi">
			<default value="0" />
			<test logic="AND" value="45">
				/controls/fuel/tank[3]/override-pump[0] == 1
				<test logic="OR">
					propulsion/tank[0]/contents-lbs le 3000
					propulsion/jettison == 1
				</test>
				propulsion/tank[3]/contents-lbs ge 7000
			</test>
		</switch>
		<switch name="propulsion/tank[3]/override-pump[1]/psi">
			<default value="0" />
			<test logic="AND" value="45">
				/controls/fuel/tank[3]/override-pump[1] == 1
				<test logic="OR">
					propulsion/tank[0]/contents-lbs le 3000
					propulsion/jettison == 1
				</test>
				propulsion/tank[3]/contents-lbs ge 7000
			</test>
		</switch>
		<switch name="propulsion/tank[1]/pump[0]/psi">
			<default value="0" />
			<test logic="AND" value="20">
				/controls/fuel/tank[1]/pump[0] == 1
				propulsion/tank[1]/contents-lbs ge 2000
			</test>
		</switch>
		<switch name="propulsion/tank[1]/pump[1]/psi">
			<default value="0" />
			<test logic="AND" value="20">
				/controls/fuel/tank[1]/pump[1] == 1
				propulsion/tank[1]/contents-lbs ge 2000
			</test>
		</switch>
		<switch name="propulsion/tank[2]/pump[0]/psi">
			<default value="0" />
			<test logic="AND" value="20">
				/controls/fuel/tank[2]/pump[0] == 1
				propulsion/tank[2]/contents-lbs ge 2000
			</test>
		</switch>
		<switch name="propulsion/tank[2]/pump[1]/psi">
			<default value="0" />
			<test logic="AND" value="20">
				/controls/fuel/tank[2]/pump[1] == 1
				propulsion/tank[2]/contents-lbs ge 2000
			</test>
		</switch>
		<switch name="propulsion/tank[3]/pump[0]/psi">
			<default value="0" />
			<test logic="AND" value="20">
				/controls/fuel/tank[3]/pump[0] == 1
				propulsion/tank[3]/contents-lbs ge 2000
			</test>
		</switch>
		<switch name="propulsion/tank[3]/pump[1]/psi">
			<default value="0" />
			<test logic="AND" value="20">
				/controls/fuel/tank[3]/pump[1] == 1
				propulsion/tank[3]/contents-lbs ge 2000
			</test>
		</switch>
		<switch name="propulsion/tank[4]/pump[0]/psi">
			<default value="0" />
			<test logic="AND" value="20">
				/controls/fuel/tank[4]/pump[0] == 1
				propulsion/tank[4]/contents-lbs ge 2000
			</test>
		</switch>
		<switch name="propulsion/tank[4]/pump[1]/psi">
			<default value="0" />
			<test logic="AND" value="20">
				/controls/fuel/tank[4]/pump[1] == 1
				propulsion/tank[4]/contents-lbs ge 2000
			</test>
		</switch>

		<!-- CROSSFEED VALVES -->

		<actuator name="propulsion/tank[1]/x-feed">
			<input>/controls/fuel/tank[1]/x-feed</input>
			<rate_limit>0.333</rate_limit>
		</actuator>
		<switch name="propulsion/tank[1]/x-feed/transit">
			<default value="1" />
			<test logic="AND" value="0">
				propulsion/tank[1]/x-feed == /controls/fuel/tank[1]/x-feed
			</test>
		</switch>

		<switch name="propulsion/tank[2]/x-feed/logic">
			<default value="/controls/fuel/tank[2]/x-feed" />
			<test logic="AND" value="0">
				/controls/fuel/tank[2]/x-feed == 1
				fcs/flaps/pos-deg ge 10
				fcs/flaps/pos-deg le 20
			</test>
		</switch>
		<actuator name="propulsion/tank[2]/x-feed">
			<input>propulsion/tank[2]/x-feed/logic</input>
			<rate_limit>0.333</rate_limit>
		</actuator>
		<switch name="propulsion/tank[2]/x-feed/transit">
			<default value="1" />
			<test logic="AND" value="0">
				propulsion/tank[2]/x-feed == propulsion/tank[2]/x-feed/logic
			</test>
		</switch>

		<switch name="propulsion/tank[3]/x-feed/logic">
			<default value="/controls/fuel/tank[3]/x-feed" />
			<test logic="AND" value="0">
				/controls/fuel/tank[3]/x-feed == 1
				fcs/flaps/pos-deg ge 10
				fcs/flaps/pos-deg le 20
			</test>
		</switch>
		<actuator name="propulsion/tank[3]/x-feed">
			<input>propulsion/tank[3]/x-feed/logic</input>
			<rate_limit>0.333</rate_limit>
		</actuator>
		<switch name="propulsion/tank[3]/x-feed/transit">
			<default value="1" />
			<test logic="AND" value="0">
				propulsion/tank[3]/x-feed == propulsion/tank[3]/x-feed/logic
			</test>
		</switch>

		<actuator name="propulsion/tank[4]/x-feed">
			<input>/controls/fuel/tank[4]/x-feed</input>
			<rate_limit>0.333</rate_limit>
		</actuator>
		<switch name="propulsion/tank[4]/x-feed/transit">
			<default value="1" />
			<test logic="AND" value="0">
				propulsion/tank[4]/x-feed == /controls/fuel/tank[4]/x-feed
			</test>
		</switch>

		<fcs_function name="propulsion/tank[12]/fuel-pressure-psi">
			<function>
				<max>
					<property>propulsion/tank[0]/override-pump[0]/psi</property>
					<property>propulsion/tank[0]/override-pump[1]/psi</property>
					<property>propulsion/tank[2]/override-pump[0]/psi</property>
					<property>propulsion/tank[2]/override-pump[1]/psi</property>
					<property>propulsion/tank[3]/override-pump[0]/psi</property>
					<property>propulsion/tank[3]/override-pump[1]/psi</property>
					<product>
						<property>propulsion/tank[1]/x-feed</property>
						<property>propulsion/tank[1]/pump[0]/psi</property>
					</product>
					<product>
						<property>propulsion/tank[1]/x-feed</property>
						<property>propulsion/tank[1]/pump[1]/psi</property>
					</product>
					<product>
						<property>propulsion/tank[2]/x-feed</property>
						<property>propulsion/tank[2]/pump[0]/psi</property>
					</product>
					<product>
						<property>propulsion/tank[2]/x-feed</property>
						<property>propulsion/tank[2]/pump[1]/psi</property>
					</product>
					<product>
						<property>propulsion/tank[3]/x-feed</property>
						<property>propulsion/tank[3]/pump[0]/psi</property>
					</product>
					<product>
						<property>propulsion/tank[3]/x-feed</property>
						<property>propulsion/tank[3]/pump[1]/psi</property>
					</product>
					<product>
						<property>propulsion/tank[4]/x-feed</property>
						<property>propulsion/tank[4]/pump[0]/psi</property>
					</product>
					<product>
						<property>propulsion/tank[4]/x-feed</property>
						<property>propulsion/tank[4]/pump[1]/psi</property>
					</product>
				</max>
			</function>
		</fcs_function>

		<!-- from INNER and CWT to crossfeed manifold-->

		<switch name="propulsion/tank[0]/external-flow/tank[12]">
			<default value="0" />
			<test logic="OR" value="1">
				propulsion/tank[0]/override-pump[0]/psi gt 0
				propulsion/tank[0]/override-pump[1]/psi gt 0
			</test>
		</switch>
		<switch name="propulsion/tank[0]/external-flow-rate/tank[12]">
			<default value="0" />
			<test logic="AND" value="20">
				propulsion/tank[0]/external-flow/tank[12] == 1
				propulsion/tank[12]/contents-lbs lt 9
			</test>
		</switch>

		<switch name="propulsion/tank[1]/external-flow/tank[8]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[1]/x-feed == 0 
				<test logic="OR">
					propulsion/tank[1]/pump[0]/psi gt 0
					propulsion/tank[1]/pump[1]/psi gt 0
				</test>
			</test>
		</switch>
		<switch name="propulsion/tank[1]/external-flow-rate/tank[8]">
			<default value="0" />
			<test logic="AND" value="20">
				propulsion/tank[1]/external-flow/tank[8] == 1
				propulsion/tank[8]/contents-lbs lt 9
			</test>
		</switch>
		<switch name="propulsion/tank[1]/external-flow/tank[12]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[1]/x-feed == 1
				propulsion/tank[12]/fuel-pressure-psi lt 45
				<test logic="OR">
					propulsion/tank[1]/pump[0]/psi gt 0
					propulsion/tank[1]/pump[1]/psi gt 0
				</test>
			</test>
		</switch>
		<switch name="propulsion/tank[1]/external-flow-rate/tank[12]">
			<default value="0" />
			<test logic="AND" value="20">
				propulsion/tank[1]/external-flow/tank[12] == 1
				propulsion/tank[12]/contents-lbs lt 9
			</test>
		</switch>

		<switch name="propulsion/tank[2]/external-flow/tank[9]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[2]/x-feed == 0
				<test logic="OR">
					propulsion/tank[2]/pump[0]/psi gt 0
					propulsion/tank[2]/pump[1]/psi gt 0
				</test>
			</test>
		</switch>
		<switch name="propulsion/tank[2]/external-flow-rate/tank[9]">
			<default value="0" />
			<test logic="AND" value="20">
				propulsion/tank[2]/external-flow/tank[9] == 1
				propulsion/tank[9]/contents-lbs lt 9
			</test>
		</switch>
		<switch name="propulsion/tank[2]/external-flow/tank[12]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[2]/x-feed == 1
				propulsion/tank[12]/fuel-pressure-psi lt 45
				<test logic="OR">
					propulsion/tank[2]/pump[0]/psi gt 0
					propulsion/tank[2]/pump[1]/psi gt 0
				</test>
				propulsion/tank[2]/override-pump[0]/psi == 0
				propulsion/tank[2]/override-pump[1]/psi == 0
			</test>
		</switch>
		<switch name="propulsion/tank[2]/external-override-flow/tank[12]">
			<default value="0" />
			<test logic="OR" value="1">
				propulsion/tank[2]/override-pump[0]/psi gt 0
				propulsion/tank[2]/override-pump[1]/psi gt 0
			</test>
		</switch>
		<switch name="propulsion/tank[2]/external-flow-rate/tank[12]">
			<default value="0" />
			<test logic="AND" value="20">
				<test logic="OR">
					propulsion/tank[2]/external-flow/tank[12] == 1
					propulsion/tank[2]/external-override-flow/tank[12] == 1
				</test>
				propulsion/tank[12]/contents-lbs lt 9
			</test>
		</switch>

		<switch name="propulsion/tank[3]/external-flow/tank[10]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[3]/x-feed == 0
				<test logic="OR">
					propulsion/tank[3]/pump[0]/psi gt 0
					propulsion/tank[3]/pump[1]/psi gt 0
				</test>
			</test>
		</switch>
		<switch name="propulsion/tank[3]/external-flow-rate/tank[10]">
			<default value="0" />
			<test logic="AND" value="20">
				propulsion/tank[3]/external-flow/tank[10] == 1
				propulsion/tank[10]/contents-lbs lt 9
			</test>
		</switch>
		<switch name="propulsion/tank[3]/external-flow/tank[12]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[3]/x-feed == 1
				propulsion/tank[12]/fuel-pressure-psi lt 45
				<test logic="OR">
					propulsion/tank[3]/pump[0]/psi gt 0
					propulsion/tank[3]/pump[1]/psi gt 0
				</test>
				propulsion/tank[3]/override-pump[0]/psi == 0
				propulsion/tank[3]/override-pump[1]/psi == 0
			</test>
		</switch>
		<switch name="propulsion/tank[3]/external-override-flow/tank[12]">
			<default value="0" />
			<test logic="OR" value="1">
				propulsion/tank[3]/override-pump[0]/psi gt 0
				propulsion/tank[3]/override-pump[1]/psi gt 0
			</test>
		</switch>
		<switch name="propulsion/tank[3]/external-flow-rate/tank[12]">
			<default value="0" />
			<test logic="AND" value="20">
				<test logic="OR">
					propulsion/tank[3]/external-flow/tank[12] == 1
					propulsion/tank[3]/external-override-flow/tank[12] == 1
				</test>
				propulsion/tank[12]/contents-lbs lt 9
			</test>
		</switch>

		<switch name="propulsion/tank[4]/external-flow/tank[11]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[4]/x-feed == 0
				<test logic="OR">
					propulsion/tank[4]/pump[0]/psi gt 0
					propulsion/tank[4]/pump[1]/psi gt 0
				</test>
			</test>
		</switch>
		<switch name="propulsion/tank[4]/external-flow-rate/tank[11]">
			<default value="0" />
			<test logic="AND" value="20">
				propulsion/tank[4]/external-flow/tank[11] == 1
				propulsion/tank[11]/contents-lbs lt 9
			</test>
		</switch>
		<switch name="propulsion/tank[4]/external-flow/tank[12]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[4]/x-feed == 1
				propulsion/tank[12]/fuel-pressure-psi lt 45
				<test logic="OR">
					propulsion/tank[4]/pump[0]/psi gt 0
					propulsion/tank[4]/pump[1]/psi gt 0
				</test>
			</test>
		</switch>
		<switch name="propulsion/tank[4]/external-flow-rate/tank[12]">
			<default value="0" />
			<test logic="AND" value="20">
				propulsion/tank[4]/external-flow/tank[12] == 1
				propulsion/tank[12]/contents-lbs lt 9
			</test>
		</switch>

		<!-- from crossfeed manifold to engine tanks -->

		<switch name="propulsion/tank[12]/external-flow/tank[8]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[12]/fuel-pressure-psi gt 0
				propulsion/tank[1]/x-feed == 1
			</test>
		</switch>
		<switch name="propulsion/tank[12]/external-flow-rate/tank[8]">
			<default value="0" />
			<test logic="AND" value="5">
				propulsion/tank[12]/external-flow/tank[8] == 1
				propulsion/tank[12]/contents-lbs gt 1
				propulsion/tank[8]/contents-lbs lt 9
			</test>
		</switch>

		<switch name="propulsion/tank[12]/external-flow/tank[9]">
			<default value="0" />
			<test logic="AND" value="1">
				propulsion/tank[12]/fuel-pressure-psi gt 0
				propulsion/tank[2]/x-feed == 1
			</test>
		</switch>
		<switch name="propulsion/tank[12]/external-flow-rate/tank[9]">
			<default value="0" />
			<test logic="AND" value="5">
				propulsion/tank[12]/external-flow/tank[9] == 1
				propulsion/tank[12]/contents-lbs gt 1
				propulsion/tank[9]/contents-lbs lt 9
			</test>
		</switch>

